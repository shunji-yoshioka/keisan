{
  "name": "keisan",
  "version": "1.0.0",
  "description": "暗算練習アプリ",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.config.js && tsc",
    "watch": "webpack --config webpack.config.js --watch",
    "dev": "concurrently \"npm run watch\" \"electron .\"",
    "package": "electron-builder build --mac --win --publish never",
    "package:mac": "electron-builder build --mac --publish never",
    "package:win": "electron-builder build --win --publish never"
  },
  "author": "Shunji Yoshioka",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.shunji.keisan",
    "productName": "暗算練習",
    "mac": {
      "category": "public.app-category.education"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ]
  }
}
